{
  "name": "ring-election",
  "version": "0.0.16",
  "description": "Leader and followers algorithm to make partitioning easy.",
  "main": "index.js",
  "engines": {
    "node": ">=10.0.0",
    "npm": ">=6.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/pioardi/ring-election.git"
  },
  "keywords": [
    "scalability",
    "concurrency",
    "distributed-systems",
    "leader-election",
    "partitioning"
  ],
  "scripts": {
    "start": "node start.js",
    "build": "npm install",
    "test": "nyc mocha --exit --timeout 10000 **/*test.js",
    "integration-test": "nyc mocha --exit --timeout 300000 **/*/integration/*.js",
    "demontest": "nodemon --exec \"npm test\"",
    "coverage": "nyc report --reporter=text-lcov --timeout 5000 **/*test.js | coveralls"
  },
  "author": "pioardi",
  "license": "MIT",
  "nyc": {
    "exclude": [
      "**/follower.js",
      "**/leader.js"
    ]
  },
  "dependencies": {
    "@reactivex/rxjs": "^5.5.11",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.11",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "coveralls": "^3.0.3",
    "expect": "^24.7.1",
    "mocha": "^5.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "mock-require": "^3.0.3",
    "nyc": "^14.0.0",
    "supertest": "^4.0.2",
    "lodash": "^4.17.11",
    "request": "2.88.0"
  }
}
